{
  "name": "hebrew-words",
  "homepage": "hhttps://maxbrokhman.github.io/learn-hebrew/",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/lodash": "^4.14.149",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "firebase": "^8.2.2",
    "formik": "^2.2.6",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "typescript": "^4.1.3"
  },
  "prettier": {
    "importSort": {
      ".js, .jsx, .ts, .tsx": {
        "style": "module"
      }
    },
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "jsx-space-before-trailing-slash": true
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && npm run change",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "change": "rm -R docs && mv 'build' 'docs'",
    "words": "git add . && git commit -m \"words updated\" && git push origin master"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
